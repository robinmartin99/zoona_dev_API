<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TargetEndpoint name="zapTest">
    <Description>Endpoint for the Zoona Testbed</Description>
    <FaultRules/>
    <PreFlow name="PreFlow">
        <Request>
            <Step>
                <Name>setTargetURL</Name>
            </Step>
            <Step>
                <Name>RF_Gatekeeper</Name>
            </Step>
        </Request>
        <Response>
            <Step>
                <Name>Set-CORS</Name>
            </Step>
        </Response>
    </PreFlow>
    <PostFlow name="PostFlow">
        <Request/>
        <Response/>
    </PostFlow>
    <Flows/>
    <HTTPTargetConnection>
        <Properties/>
        <URL>https://openapi-gateway.uat.zoona.network/</URL>
    </HTTPTargetConnection>
</TargetEndpoint>